# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.html]
indent_style = space
indent_size = 2

[*.nunj]
indent_style = space
indent_size = 2

[*.ts]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2


[*.scss]
indent_style = space
indent_size = 2

[*.css]
indent_style = space
indent_size = 2
